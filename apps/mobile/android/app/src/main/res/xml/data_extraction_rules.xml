<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Specify what data to backup to the cloud -->
        <include domain="file" path="."/>
        <exclude domain="file" path="device_specific_files/"/>
    </cloud-backup>
    <device-transfer>
        <!-- Specify what data to transfer between devices -->
        <include domain="file" path="."/>
        <exclude domain="file" path="device_specific_files/"/>
    </device-transfer>
</data-extraction-rules>