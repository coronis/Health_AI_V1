# ⚠️ DEMO ENVIRONMENT - FOR LOCAL DEVELOPMENT ONLY
# This file contains demo keys and should NEVER be used in production

# n8n Core Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=demo-admin
N8N_BASIC_AUTH_PASSWORD=demo-password-123

# Database
DB_TYPE=postgres
DB_POSTGRESDB_HOST=localhost
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n_dev
DB_POSTGRESDB_USER=demo-user
DB_POSTGRESDB_PASSWORD=demo-password

# Encryption
N8N_ENCRYPTION_KEY=demo-encryption-key-for-development-only

# Webhook Configuration
WEBHOOK_URL=http://localhost:5678/
N8N_PROTOCOL=http
N8N_HOST=localhost
N8N_PORT=5678

# External Service URLs
HEALTHCOACHAI_API_URL=http://localhost:8080
HEALTHCOACHAI_API_KEY=demo-api-key

# AI Provider Keys (Demo Keys)
OPENAI_API_KEY=sk-demo-openai-key
ANTHROPIC_API_KEY=sk-ant-demo-anthropic-key
GOOGLE_APPLICATION_CREDENTIALS=/opt/demo-credentials.json

# Security
N8N_JWT_AUTH_ACTIVE=false
N8N_PUBLIC_API_DISABLED=false

# Logging
N8N_LOG_LEVEL=debug
N8N_LOG_OUTPUT=console

# Performance
EXECUTIONS_PROCESS=main
EXECUTIONS_TIMEOUT=3600
EXECUTIONS_TIMEOUT_MAX=7200